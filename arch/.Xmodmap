! remove Lock = Caps_Lock
! add Lock = Escape
! keysym Caps_Lock = Escape
! keysym Escape = Caps_Lock

!Swap right control and right alt
! remove Control = Control_R
! remove Mod1 = Alt_R
! keycode 105 = Alt_R
! keycode 108 = Control_R
! add Control = Control_R
! add Mod1 = Alt_R

! Swap left control and left alt
! Super_L=133
! Super_R=134
! Alt_L=64
! Control_R=105
! remove Control = Control_R
! remove Mod1 = Alt_L
! remove Mod4 = Super_R Super_L
! keycode 133 = Alt_L
! keycode 64 = Super_L
! keycode 105 = Super_R
! keycode 134 = Control_R
! add Control = Control_R
! add Mod1 = Alt_L
! add Mod4 = Super_R Super_L
! Super_R -> alt r
remove Mod4 = Super_R
remove Mod1 = Alt_L
keycode 64 = Super_R
keycode 134 = Alt_L
add Mod1 = Alt_L
add Mod4 = Super_R
