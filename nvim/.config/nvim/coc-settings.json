{
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "ﯦ ",
  "diagnostic.level": "warning",
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.enableHighlightLineNumber": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.virtualText": false,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.messageDelay": 10,

  "tsserver.trace.server": "verbose",

  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "json",
    "yml",
    "typescript",
    "typescriptreact",
    "html",
    "vim"
  ],

  "javascript.validate.enable": true,
  "typescript.showUnused": true,
  "javascript.showUnused": true,

  "snippets.ultisnips.enable": false,
  "snippets.snipmate.enable": true,
  "snippets.textmateSnippetsRoots": ["~/.vsnip"],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["typescript", "javascript"],
    "typescriptreact": ["javascript", "typescript", "javascriptreact"]
  },

  "prettier.eslintIntegration": false,

  "eslint.autoFixOnSave": false,

  "suggest.completionItemKindLabels": {
    "keyword": " [keyword]",
    "variable": " [variable]",
    "value": " [value]",
    "operator": " [operator]",
    "function": " [function]",
    "reference": "渚[reference]",
    "constant": " [constant]",
    "method": " [method]",
    "struct": "פּi [struct]",
    "class": " [class]",
    "interface": " [interface]",
    "text": " [text]",
    "enum": " [enum]",
    "enumMember": " [enumMember]",
    "module": "  [module]",
    "color": " [color]",
    "property": " [property]",
    "field": "料[field]",
    "unit": " [unit]",
    "event": "鬒 [event]",
    "file": " [file]",
    "folder": " [folder]",
    "snippet": " [snippet]",
    "typeParameter": " [typeParameter]",
    "default": " [default]"
  },

  "java.format.enabled": true,
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  "java.format.settings.profile": "GoogleStyle",
  "java.saveActions.organizeImports": false,
  "java.trace.server": "verbose"
}
